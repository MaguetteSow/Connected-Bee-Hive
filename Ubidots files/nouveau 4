while True:    temperature = -23.3    # Your sensor value    humidity = 40          # Your sensor value    # > = big-endian    # h = Temperature (2 bytes, 16 bits, signed) Range: âˆ’32,768 to 32,767    # B = Humidity (1 byte, 8 bits, unsigned) Range: 0 to 255    payload = struct.pack('>hB', int(temperature * 100), int(humidity))    s.send(payload)    print('sent temperature:', temperature)    print('sent humidity:', humidity)    time.sleep(900)     # wait 900 seconds (15 minutes) before sending again